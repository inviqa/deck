{
  "name": "startup_theme",
  "version": "1.0.0",
  "description": "Dependencies for the startup_theme theme",
  "private": true,
  "scripts": {
    "build": "npm run build:webpack && npm run build:fractal",
    "build:fractal": "fractal build",
    "build:webpack": "webpack",
    "prebuild:production": "npm run lint",
    "build:production": "NODE_ENV=production npm run build",
    "lint": "npm run lint:styles && npm run lint:scripts",
    "lint:scripts": "eslint ./assets/src --format=node_modules/eslint-codeframe-formatter",
    "lint:styles": "stylelint \"./src/components/**/*.scss\"",
    "start:fractal": "fractal start --sync --port 3000",
    "start:webpack": "webpack -w",
    "test": "jest ./src/components"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@frctl/fractal": "^1.1.7",
    "@frctl/mandelbrot": "^1.2.0",
    "@frctl/twig": "^1.0.0-beta.5",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "dom-testing-library": "^3.17.1",
    "eslint": "^5.3.0",
    "eslint-codeframe-formatter": "^1.0.2",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.2",
    "file-loader": "^3.0.1",
    "imagemin-pngquant": "^7.0.0",
    "imagemin-svgo": "^7.0.0",
    "imagemin-webpack": "^4.1.0",
    "jest": "^24.5.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "resolve-url-loader": "^3.0.1",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.10.1",
    "stylelint-a11y": "^1.1.10",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.5.4",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {}
}
